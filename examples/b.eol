public class Multiplication {
    public static void main() {
        long sum = 0;
        long i = 1;
        while (i <= 1000) {
            long j = 1;
            while (j <= 1000) {
                sum = sum + i * j;
                j = j + 1;
            }
            i = i + 1;
        }
        print(sum);
    }
}