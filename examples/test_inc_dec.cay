public class TestIncDec {
    public static void main() {
        // 测试自增和自减运算符
        int a = 5;
        int b = 10;
        
        println("初始值: a = " + a + ", b = " + b);
        
        // 测试前缀自增
        int c = ++a;
        println("++a 后: a = " + a + ", c = " + c);
        
        // 测试后缀自增
        int d = b++;
        println("b++ 后: b = " + b + ", d = " + d);
        
        // 测试前缀自减
        int e = --a;
        println("--a 后: a = " + a + ", e = " + e);
        
        // 测试后缀自减
        int f = b--;
        println("b-- 后: b = " + b + ", f = " + f);
        
        // 测试复合赋值运算符
        int x = 100;
        println("\n初始值: x = " + x);
        
        x += 10;
        println("x += 10: x = " + x);
        
        x -= 20;
        println("x -= 20: x = " + x);
        
        x *= 2;
        println("x *= 2: x = " + x);
        
        x /= 3;
        println("x /= 3: x = " + x);
        
        x %= 5;
        println("x %= 5: x = " + x);
        
        // 测试在循环中使用自增
        println("\n循环中使用自增:");
        for (int i = 0; i < 5; i++) {
            print(i + " ");
        }
        println("");
        
        // 测试在循环中使用自减
        println("\n循环中使用自减:");
        int j = 5;
        while (j > 0) {
            print(j + " ");
            j--;
        }
        println("");
        
        println("\n所有测试完成!");
    }
}
